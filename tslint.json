{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "align": [
            true,
            "elements",
            "members",
            "parameters",
            "statements"
        ],
        "comment-format": [
            true,
            "check-space",
            "check-uppercase",
            "allow-trailing-lowercase"
        ],
        "curly": true,
        "eofline": false,
        "max-line-length": [
            true,
            160
        ],
        "no-consecutive-blank-lines": true,
        "no-console": [
            true,
            "log",
            "error"
        ],
        "no-debugger": true,
        "no-duplicate-switch-case": true,
        "no-empty": true,
        "no-shadowed-variable": true,
        "no-string-literal": false,
        "no-trailing-whitespace": [
            true,
            "ignore-comments"
        ],
        "object-literal-sort-keys": false,
        "only-arrow-functions": false,
        "prefer-const": true,
        "prefer-for-of": false,
        "quotemark": true,
        "radix": false,
        "semicolon": true,
        "space-before-function-paren": [
            true,
            {
                "anonymous": "always",
                "named": "never",
                "asyncArrow": "always"
            }
        ],
        "typedef": [
            true,
            "parameter",
            "property-declaration",
            "variable-declaration"
        ],
        "trailing-comma": false,
        "triple-equals": true,
        "unnecessary-else": true,
        "variable-name": [
            true,
            "check-format",
            "ban-keywords"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-module",
            "check-type",
            "check-typecast",
            "check-type-operator",
            "check-preblock"
        ]
    },
    "rulesDirectory": []
}